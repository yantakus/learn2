{
  "name": "@learn/server",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node -r tsconfig-paths/register --transpile-only src/schema",
    "postinstall": "npm -s run generate",
    "dev": "ts-node-dev -r tsconfig-paths/register --no-notify --respawn --transpileOnly src/server",
    "dev-generate": "prisma generate --watch",
    "migrate-save": "npx prisma migrate save --experimental",
    "migrate-up": "npx prisma migrate up --experimental"
  },
  "dependencies": {
    "@nexus/schema": "^0.13.1",
    "@prisma/client": "2.0.0-beta.2",
    "firebase": "^7.13.2",
    "firebase-admin": "^8.10.0",
    "graphql": "14.6.0",
    "graphql-shield": "5.7.3",
    "graphql-tag": "^2.10.3",
    "graphql-yoga": "1.18.3",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "nexus": "^0.12.0",
    "nexus-prisma": "^0.12.0",
    "react-firebaseui": "^4.1.0"
  },
  "devDependencies": {
    "@prisma/cli": "2.0.0-beta.2",
    "@types/node": "12.12.34",
    "dotenv": "^8.2.0",
    "ts-node": "8.8.1",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.8.3"
  }
}
