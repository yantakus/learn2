{
  "name": "Learn",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nexus build && next build",
    "migrate": "yarn -s prisma migrate save --experimental && yarn -s prisma migrate up --experimental",
    "deploy:preview": "now",
    "deploy:prod": "now --prod",
    "next:inspect": "NODE_OPTIONS='--inspect' next",
    "nexus:check": "nexus build --entrypoint pages/api/graphql.ts && nexus build --entrypoint pages/api/playground.ts",
    "nexus": "nexus dev --reflection --entrypoint pages/api/graphql.ts",
    "dev": "concurrently \"yarn next\" \"yarn nexus\"",
    "start": "next start",
    "test": "nexus build"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.4",
    "@apollo/react-ssr": "^3.1.4",
    "@chakra-ui/core": "0.8.0",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@prisma/client": "2.0.0-beta.4",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.17",
    "apollo-link-schema": "^1.2.5",
    "concurrently": "^5.2.0",
    "cookie-parser": "^1.4.5",
    "emotion-theming": "^10.0.27",
    "firebase": "^7.13.2",
    "firebase-admin": "^8.10.0",
    "formik": "^2.1.4",
    "graphql": "14.6.0",
    "graphql-tag": "^2.10.3",
    "graphql-type-json": "^0.3.1",
    "graphql-yoga": "1.18.3",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.15",
    "next": "9.3.5",
    "nexus": "0.22.0",
    "nexus-plugin-prisma": "0.8.0",
    "nexus-plugin-shield": "^0.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-firebaseui": "^4.1.0",
    "react-select": "^3.1.0"
  },
  "devDependencies": {
    "@prisma/cli": "2.0.0-beta.7",
    "@types/cookie-parser": "^1.4.2",
    "@types/graphql-type-json": "^0.3.2",
    "@types/lodash": "4.14.108",
    "@types/node": "12.12.34",
    "@types/react": "^16.9.32",
    "@types/react-select": "^3.0.13",
    "dotenv": "^8.2.0"
  }
}
