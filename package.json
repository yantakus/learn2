{
  "name": "graphql-auth",
  "license": "MIT",
  "scripts": {
    "start-server": "node server/dist/server",
    "clean-server": "cd server && rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "cd server && prisma generate",
    "generate:nexus": "ts-node --transpile-only server/src/schema",
    "postinstall": "npm -s run generate",
    "dev-server": "ts-node-dev --no-notify --respawn --transpileOnly server/src/server",
    "dev-client": "cd client && next dev"
  },
  "dependencies": {
    "@prisma/client": "2.0.0-beta.1",
    "bcryptjs": "2.4.3",
    "cookie-session": "^1.4.0",
    "firebase": "^7.13.2",
    "firebase-admin": "^8.10.0",
    "graphql": "14.6.0",
    "graphql-shield": "5.7.3",
    "graphql-yoga": "1.18.3",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "^4.17.15",
    "next": "^9.3.4",
    "nexus": "0.12.0",
    "nexus-prisma": "0.11.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-firebaseui": "^4.1.0"
  },
  "devDependencies": {
    "@prisma/cli": "2.0.0-beta.1",
    "@types/bcryptjs": "2.4.2",
    "@types/jsonwebtoken": "8.3.8",
    "@types/node": "12.12.34",
    "@types/react": "^16.9.32",
    "@types/ws": "7.2.3",
    "dotenv": "^8.2.0",
    "ts-node": "8.8.1",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.8.3"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
